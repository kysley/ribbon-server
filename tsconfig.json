{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": [
      "esnext"
    ],
    "strict": true,
    "rootDir": "src",
    "typeRoots": ["node_modules/@types", "types"],
    "noEmit": true,
    "plugins": [
      {
        "name": "nexus/typescript-language-service"
      }
    ]
  },
  "include": [
    "src",
    "types.d.ts"
  ]
}
